        },
        "password": {
          "type": "string"
        },
        "username": {
          "type": "string"
        },
        "given_name": {
          "type": "string"
        },
        "family_name": {
          "type": "string"
        },
        "nickname": {
          "type": "string"
        },
        "picture": {
          "type": "string"
        }
      }
    },
    "templateTodo": {
      "type": "object",
      "properties": {
        "id": {
          "type": "string"
        },
        "text": {
          "type": "string"
        },
        "author": {
          "type": "string"
        },
        "timestamp": {
          "type": "string"
        }
      }
    },
    "templateTokenRequest": {
      "type": "object",
      "properties": {
